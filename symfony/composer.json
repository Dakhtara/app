{
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "excelwebzone/recaptcha-bundle": "^1.5",
    "fabpot/goutte": "^3.2",
    "google/apiclient": "^2.0",
    "guzzlehttp/guzzle": "^6.3",
    "league/csv": "^9.1",
    "mpdf/mpdf": "^7.1",
    "nexmo/client": "^1.3",
    "ninsuo/symfony-collection": "^2.1",
    "pagerfanta/pagerfanta": "^2.0",
    "ramsey/uuid": "^3.7",
    "redcall-io/password-login-bundle": "^0.9.0",
    "sensio/framework-extra-bundle": "^5.1",
    "sentry/sentry-symfony": "^2.0",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "^4.1",
    "symfony/console": "^4.1",
    "symfony/dotenv": "^4.1",
    "symfony/expression-language": "^4.1",
    "symfony/flex": "^1.0",
    "symfony/form": "^4.1",
    "symfony/framework-bundle": "^4.1",
    "symfony/lts": "^4@dev",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "*",
    "symfony/process": "^4.1",
    "symfony/security-bundle": "^4.1",
    "symfony/security-csrf": "^4.1",
    "symfony/serializer-pack": "*",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/twig-bridge": "^4.1",
    "symfony/twig-bundle": "^4.1",
    "symfony/validator": "^4.1",
    "symfony/web-link": "^4.1",
    "symfony/webpack-encore-bundle": "^1.5",
    "symfony/yaml": "^4.1",
    "white-october/pagerfanta-bundle": "^1.2"
  },
  "require-dev": {
    "redcall-io/sandbox-bundle": "dev-master",
    "symfony/debug-pack": "*",
    "symfony/dotenv": "^4.1",
    "symfony/maker-bundle": "^1.0",
    "symfony/profiler-pack": "*",
    "symfony/test-pack": "^1.0",
    "symfony/web-server-bundle": "^4.1"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "platform": {
      "php": "7.1.23"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Bundles\\": "bundles/",
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts",
      "Fuz\\Symfony\\Collection\\ScriptHandler::postInstall"
    ],
    "post-update-cmd": [
      "@auto-scripts",
      "Fuz\\Symfony\\Collection\\ScriptHandler::postUpdate"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false
    }
  }
}
